page {
  background-color: #ffffff;
}
.shouhou-goods-box {
  padding-bottom: 20rpx;
}
.shouhou-goods-box .order-title {
  width: 90%;
  margin: 0 auto;
  display: flex;
  height: 80rpx;
  align-items: center;
  color: #27323f;
}
.shouhou-goods-box .order-title text {
  color: #7b8196;
  font-size: 24rpx;
  padding-left: 10rpx;
}
.shouhou-goods-box .order-goods-list-box {
  width: 95%;
  margin: 0 auto;
  border-bottom: 2rpx solid #f5f5f5;
}
.shouhou-goods-box .order-goods-list-box .favorite-list-img {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.shouhou-goods-box .order-goods-list-box .favorite-list-img image {
  width: 80%;
  height: 80%;
  border-radius: 4rpx;
}
.shouhou-goods-box .order-goods-list-box .times {
  font-size: 24rpx;
  color: #7b8196;
}
.shouhou-goods-box .order-goods-list-box .price {
  color: #a18d65;
  font-size: 30rpx;
}
.shouhou-goods-box .order-goods-list-box .goods-name {
  color: #27323f;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 28rpx;
}
.shouhou-goods-box .order-goods-list-box .goods-info {
  color: #7b8196;
  font-size: 24rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.tuikuan-content .order-title {
  width: 90%;
  margin: 0 auto;
  display: flex;
  height: 80rpx;
  align-items: center;
  color: #27323f;
}
.tuikuan-content .order-title text {
  color: #7b8196;
  font-size: 24rpx;
  padding-left: 10rpx;
}
.tuikuan-content .order-info {
  background-color: #f9f9f9;
  width: 90%;
  margin: 0 auto;
  border-radius: 6rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  font-size: 26rpx;
  color: #27323f;
}
.tuikuan-content .order-info .name {
  color: #7b8196;
}
.tuikuan-content .kanjia-title {
  padding-top: 50rpx;
}
.tuikuan-content .tuikuan-box .list {
  border-bottom: 2rpx solid #f5f5f5;
  width: 90%;
  margin: 0 auto;
  margin-top: 10rpx;
}
.tuikuan-content .tuikuan-box .list .name {
  font-size: 26rpx;
  color: #27323f;
}
.tuikuan-content .tuikuan-box .list input {
  width: 100%;
  color: #27323f;
}
.tuikuan-content .tuikuan-box .list textarea {
  width: 100%;
  color: #27323f;
}
.tuikuan-content .tuikuan-box .list picker {
  width: 100%;
}
.tuikuan-content .tuikuan-box .list picker .picker-box {
  width: 100%;
  height: 80rpx;
  color: #27323f;
  display: flex;
  align-items: center;
}
.tuikuan-content .tuikuan-box .list picker .picker-box .picker {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
}
.tuikuan-content .tuikuan-box .list .seletap {
  position: absolute;
  border: 2rpx solid #e2e2e2;
  color: #27323f;
  border-radius: 6rpx;
  font-size: 24rpx;
  width: 120rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  right: 0;
  bottom: 26rpx;
  z-index: 9999;
}
.tuikuan-content .tuikuan-box .list-textarea {
  padding-top: 20rpx;
  margin-top: 0;
}
.images-box {
  width: 90%;
  margin: 0 auto;
  padding-top: 10rpx;
  display: flex;
  align-items: center;
  height: 120rpx;
}
.images-box .images-add {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  width: 100rpx;
  height: 100rpx;
}
.images-box image {
  width: 100rpx;
  height: 100rpx;
  margin-right: 10rpx;
  overflow: hidden;
}
.kefu-box {
  width: 90%;
  height: 150rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  border-radius: 6rpx;
  border: 2rpx solid #f5f5f5;
}
.kefu-box .kefu-title {
  color: #7b8196;
  font-size: 26rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
}
.kefu-box button,
.kefu-box .button {
  border: 2rpx solid #e2e2e2;
  color: #27323f;
  padding: 12rpx 40rpx;
  border-radius: 6rpx;
  font-size: 26rpx;
  background-color: #fff;
  line-height: 40rpx;
}
.kefu-box button::after {
  border: 0;
}
form button {
  width: 90%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding-left: 0;
  padding-right: 0;
  margin-top: 80rpx;
  border-radius: 4rpx;
  background-color: #ffd305;
  color: #27323f;
  font-size: 28rpx;
  margin-bottom: 80rpx;
}
form button::after {
  border: 0;
}
